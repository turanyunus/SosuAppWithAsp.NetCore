@model PostListModel
@{
    ViewData["Title"] = "CategorySubjectList";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<div class="row" style="width:1200px">
    <div class="panel" style="width:500px; float:right;">
        <div class="panel-heading">
            <h3 class="panel-title">Kategori Listesi</h3>
        </div>
        <div class="panel-body">
            <table class="table table-striped">
                <thead>
                    <tr>
                        <th>Id</th>
                        <th>Ismi</th>
                    </tr>
                </thead>
                <tbody>
                    @foreach (var item in Model.Category)
                    {
                        <tr>
                            <th>@item.Id</th>
                            <th>@item.Name</th>
                        </tr>
                    }
                </tbody>
            </table>
        </div>
    </div>
    <div class="panel" style="width:500px">
        <div class="panel-heading">
            <h3 class="panel-title">Konular Listesi</h3>
        </div>
        <div class="panel-body">
            <table class="table table-striped">
                <thead>
                    <tr>
                        <th>Id</th>
                        <th>Ismi</th>
                    </tr>
                </thead>
                <tbody>
                    @foreach (var item in Model.Subject)
                    {
                        <tr>
                            <th>@item.Id</th>
                            <th>@item.Name</th>
                        </tr>
                    }
                </tbody>
            </table>
        </div>
    </div>
</div>